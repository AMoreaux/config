{
  "rules": [
   {
      "name": "Design Tokens / Vectors",
      "path": "./output/vectors",
      "filter": {
        "types": [
          "vector"
        ]
      },
      "parsers": [
        {
          "name": "kebabcasify"
        },
        {
          "name": "svgo",
          "options": {
            "svgo": {
              "js2svg": {
                "pretty": true
              }
            }
          }
        }
      ]
    },
    {
      "name": "Design Tokens / Bitmap",
      "path": "./output/bitmap",
      "filter": {
        "types": [
          "bitmap"
        ]
      },
      "parsers": []
    }
  ]
}
      


